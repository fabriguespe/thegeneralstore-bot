    "start": "node dist/index.js"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@xmtp/message-kit": "^1.0.0-beta.2",
    "openai": "^4.67.2"
  },
  "engines": {
    "node": ">=20"
  }
}
