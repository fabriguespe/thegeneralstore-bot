    "@redis/client": "^1.6.0",
    "@types/jest": "^29.5.13",
    "@xmtp/message-kit": "^1.1.5-beta.5",
    "axios": "^1.7.7",
    "jest": "^29.7.0",
    "lowdb": "^7.0.1",
    "openai": "^4.67.2",
    "ts-jest": "^29.2.5"
  },
  "engines": {
    "node": ">=20"
  }
}
